{
  "name": "okanjo-app-server-docs",
  "version": "1.1.0",
  "description": "Generate API/route docs automatically",
  "main": "DocService.js",
  "scripts": {
    "clean": "rm -rf .nyc_output coverage",
    "test": "node_modules/.bin/mocha -R spec test",
    "cover": "node_modules/.bin/nyc node_modules/mocha/bin/_mocha --exit",
    "lint": "node_modules/.bin/eslint .",
    "report": "npm run cover && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okanjo/okanjo-app-server-docs.git"
  },
  "keywords": [
    "Okanjo",
    "OkanjoApp",
    "HAPI",
    "Docs",
    "Markdown",
    "API"
  ],
  "author": "Okanjo Partners Inc",
  "license": "MIT",
  "devDependencies": {
    "async": "^2.6.0",
    "eslint": "^4.10.0",
    "joi": "^9.2.0",
    "mocha": "^4.0.1",
    "needle": "^2.0.1",
    "nyc": "^11.3.0",
    "okanjo-app": "^1.0.2",
    "okanjo-app-server": "^1.0.0",
    "hapi-auth-basic-key": "^4.2.0-key",
    "should": "^13.1.3"
  },
  "peerDependencies": {
    "okanjo-app": ">= 1",
    "okanjo-app-server": ">= 1"
  },
  "dependencies": {
  },
  "nyc": {
    "reporter": [
      "text-summary",
      "html",
      "lcovonly"
    ]
  }
}
